{
  "name": "agrosynx",
  "version": "1.0.0",
  "description": "AgroSynx - Agricultural Services Platform",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "cd api && npm run dev",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build:vercel",
    "build:api": "cd api && npm run build",
    "start": "cd api && npm start",
    "install:all": "npm install && cd api && npm install && cd ../client && npm install",
    "vercel-build": "npm run build:client"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "vite": "^4.5.5",
    "@vitejs/plugin-react": "^4.3.2"
  },
  "keywords": ["agriculture", "services", "platform"],
  "author": "Harshit Bhardwaj",
  "license": "MIT"
} 